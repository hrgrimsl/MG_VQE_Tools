cd MG_VQE_Tools
source ~/.bashrc
for j in {0..0}
    do
    python beh2GO.py beh2GO $j
    python beh2sGO.py beh2sGO $j
    python beh2ucc.py beh2ucc $j
    for i in {0..59}
       do
       python Driver.py beh2GO_$i beh2GO_$i-$j &
       python Driver.py beh2sGO_$i beh2sGO_$i-$j &
       python Driver.py beh2ucc_$i beh2ucc_$i-$j &
       sleep 1
    done
    wait
    sleep 1
done
wait



