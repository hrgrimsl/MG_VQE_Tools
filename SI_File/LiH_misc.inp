cd MG_VQE_Tools
source ~/.bashrc
for j in {0..0}
    do
    python lihGO.py lihGO $j
    python lihsGO.py lihsGO $j
    python lihucc.py lihucc $j
    for i in {0..29}
       do
       python Driver.py lihGO_$i lihGO_$i-$j &
       python Driver.py lihsGO_$i lihsGO_$i-$j &
       python Driver.py lihucc_$i lihucc_$i-$j &
       sleep 1
    done
    wait
    sleep 1
done
wait



